<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;de.hybris.training.facades.product.data.GenderData>"/>
		<property name="vehicleRegistrationNumber" type="String"/>
		<property name="driver" type="String"/>
		<property name="dealer" type="java.util.List&lt;de.hybris.training.facades.product.data.VehicleList>"/>
	</bean>

	<bean class="de.hybris.training.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>
	<bean class="de.hybris.training.facades.product.data.VehicleList">
		<property name="vehicleDescription" type="String"/>
	</bean>
	<bean class="de.hybris.platform.commercefacades.driver.data.DriverData">
		<property name="id" type="String"/>
		<property name="driverName" type="String"/>
		<property name="driverExperience" type="Integer"/>
	</bean>
	<bean class="de.hybris.platform.commercefacades.dealer.data.DealerData">
		<property name="id" type="String"/>
		<property name="dealerName" type="String"/>
		<property name="dealerAddress" type="String"/>
	</bean>
</beans>
